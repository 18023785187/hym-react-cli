const path = require('path')
const { override } = require('customize-cra')
<%- data.modules.join('\n') %>

module.exports = override(
    aliasConfig,
    <%- data.funcName.join('\n') %>
)
// 路径别名
function aliasConfig(config) {
    config.resolve.alias = {
        ...config.resolve.alias,
        "@": path.resolve('./', 'src'),
        "pages": "@/pages",
        "components": "@/components",
        "assets": "@/assets",
        "api": "@/api",
        "utils": "@/utils",
    }
    return config
}
